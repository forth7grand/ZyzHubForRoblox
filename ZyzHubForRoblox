local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "ZyzzV2.0.2", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})
